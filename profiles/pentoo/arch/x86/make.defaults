CFLAGS="-Os -march=pentium-m -mtune=nocona -pipe -fomit-frame-pointer"
CXXFLAGS="${CFLAGS}"
FFLAGS="${CFLAGS}"
FCFLAGS="${CFLAGS}"

#we build for pentium m, and that has sse2
USE="${USE} sse sse2 mmx cpu_flags_x86_sse2"

QEMU_SOFTMMU_TARGETS="arm i386"
QEMU_USER_TARGETS="arm i386"

UNICORN_TARGETS="arm aarch64 x86"
